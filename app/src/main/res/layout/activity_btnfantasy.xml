<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/btnfantasy"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#090909"
    tools:context=".btnfantasy">

    <ImageView
        android:id="@+id/fan4"
        android:layout_width="147dp"
        android:layout_height="150dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toEndOf="@+id/fan3"
        app:layout_constraintTop_toBottomOf="@+id/fan2"
        app:srcCompat="@drawable/fant" />

    <ImageView
        android:id="@+id/fan6"
        android:layout_width="147dp"
        android:layout_height="150dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="42dp"
        app:layout_constraintStart_toEndOf="@+id/fan5"
        app:layout_constraintTop_toBottomOf="@+id/fan4"
        app:srcCompat="@drawable/fantas" />

    <ImageView
        android:id="@+id/fan3"
        android:layout_width="147dp"
        android:layout_height="150dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fan1"
        app:srcCompat="@drawable/fan" />

    <ImageView
        android:id="@+id/fan5"
        android:layout_width="147dp"
        android:layout_height="150dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="42dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fan3"
        app:srcCompat="@drawable/fanta" />

    <ImageView
        android:id="@+id/fan1"
        android:layout_width="147dp"
        android:layout_height="150dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="52dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backfan"
        app:srcCompat="@drawable/f" />

    <ImageView
        android:id="@+id/fan2"
        android:layout_width="147dp"
        android:layout_height="150dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toEndOf="@+id/fan1"
        app:layout_constraintTop_toBottomOf="@+id/Fantext"
        app:srcCompat="@drawable/fa" />

    <ImageButton
        android:id="@+id/backfan"
        android:layout_width="158dp"
        android:layout_height="63dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="52dp"
        android:backgroundTint="#090909"
        app:layout_constraintBottom_toTopOf="@+id/fan1"
        app:layout_constraintEnd_toStartOf="@+id/Fantext"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/abc_vector_test" />

    <TextView
        android:id="@+id/Fantext"
        android:layout_width="174dp"
        android:layout_height="64dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="18dp"
        android:text="     Fantasy"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/backfan"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>